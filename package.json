{
  "name": "react-admin-starter",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/api": {
      "target": "http://localhost:3001",
      "pathRewrite": {
        "^/api": "/"
      }
    }
  },
  "dependencies": {
    "babel-eslint": "^8.2.3",
    "bootstrap": "^4.1.1",
    "font-awesome": "^4.7.0",
    "json-schema": "^0.2.3",
    "json-server": "^0.12.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
